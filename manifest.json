{
    "manifest_version":2,
    "name":"Blockad (no more ads)",
    "description":"Free extension for youtube users, Blockad takes care of removing annoying ads for you",
    "version":"1.0",
    "content_scripts":[{
        "js":["script.js"],
        "runt_at": "document_idle",
        "matches":["<all_urls>"]
    }],
    "browser_action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
      },
    "permissions":[
        "tabs",
        "activeTab"
    ]
}