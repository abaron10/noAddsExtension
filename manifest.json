{
    "manifest_version":2,
    "name":"Blockad",
    "description":"",
    "version":"1.0",
    "content_scripts":[{
        "js":["script.js"],
        "runt_at": "document_idle",
        "matches":["<all_urls>"]
    }],
    "browser_action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
      },
    "permissions":[
        "tabs",
        "activeTab"
    ]
}